<%- include("partials/header");%>
    <h1>Home</h1>
    <p>
        <%= content%>
    </p>
    <% posts.forEach((post, postId)=> { %>
        <a style="color: inherit; text-decoration: none" href=<%="/posts/" + postId%>>
            <h3>
                <%= post.title %>
            </h3>
        </a>
        <p>
            <%= post.body %>
        </p>
        <% })%>
            <%- include("partials/footer");%>